<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.pj.pet.board.notice.NoticeMapper">


	<select id="getList" resultType="NoticeVO">
		SELECT * FROM NOTICE
	</select>

	<insert id="setAdd" parameterType="NoticeVO" useGeneratedKeys="true"  keyProperty="num">
		INSERT INTO NOTICE(NUM, ID, TITLE, CONTENTS, REGDATE)
		VALUES(#{num}, #{id}, #{title}, #{contents}, now())
	</insert>




</mapper>