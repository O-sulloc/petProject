<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.pj.pet.order.OrderMapper">
	
	<insert id="setAdd" parameterType="OrderVO" useGeneratedKeys="true" keyProperty="orderNum">
		INSERT INTO ORDER(ORDERNUM,ID,ORDERPRICE,ORDERAMOUNT,PAYCHECK)
		VALUES(#{orderNum},#{id},#{orderPrice},#{orderAmount},1)
	</insert>

</mapper>